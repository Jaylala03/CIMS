            success: function (data) {
                if (data.length > 0) {
                    $("#NatureofIncidentTable").html("");
                    $('#NatureofIncidentTable').append(data);
                }
                else { $("#NatureofIncidentTable").html(""); }
            },
            error: function () {
                console.log('error');
            }
        });
    }

