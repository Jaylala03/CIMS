            var tablenumber = $('#CashoutTableNumber').val();

            $.ajax({
                url: '@Url.Content("~/Setting/Setting/MasterCashoutTableNumber_IU")',
                data: JSON.stringify({ 'ID': id, 'TableNumber': tablenumber }),
                type: 'Post',
                dataType: 'json',
                contentType: 'application/json',
                success: function (data) {
                    if (data == "success") {
                        $('#msgDiv').html('<div class="alert alert-success">Table Number saved Successfully.</div>');

                        setTimeout(function () {
                            $('#msgDiv').fadeOut();
                        }, 5000);
                        window.scrollTo(0, 0);

                    } else {
                        $('#msgDiv').html('<div class="alert alert-danger">Some error Occurred, please try again later.</div>');
                    }
                    resetCashoutTableNumber(); // reset CashoutTableNumber
                    CashoutTableNumber(); // Bind CashoutTableNumber
                },
                error: function () {
                    console.log('error');
                }
            });
        }
    }

    function DeleteCashoutTableNumber(id) {
        var x = DeleteConfirm('@ViewBag.Action');
        if (x) {
            $.ajax({
                url: '@Url.Content("~/Setting/Setting/MasterCashoutTableNumber_Delete")',
                data: JSON.stringify({ 'ID': id }),
                type: 'Post',
                dataType: 'json',
                contentType: 'application/json',
                success: function (data) {
                    if (data == "success") {
                        $('#msgDiv').html('<div class="alert alert-success">Table Number has been deleted Successfully.</div>');
                        setTimeout(function () {
                            $('#msgDiv').fadeOut();
                        }, 5000);
                        window.scrollTo(0, 0);

                    } else {
                        $('#msgDiv').html('<div class="alert alert-danger">Some error Occurred, please try again later.</div>');
                    }
                    resetCashoutTableNumber(); // reset CashoutTableNumber
                    CashoutTableNumber(); // Bind CashoutTableNumber
                },
                error: function () {
                    console.log('error');
                }
            });
        }
    }
    // End CashoutTableNumber
</script>