                                                        <label class="control-label" for="rbPersonnel">Employee</label>
                                                        <input type="radio" name="Radio1" value="3" id="rbPersonnel">
                                                        <label class="control-label" for="rbPersonnel">General</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                    <div class="col-md-12">
