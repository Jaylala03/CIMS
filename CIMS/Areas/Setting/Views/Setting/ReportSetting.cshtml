                                                                <button type="submit" id="uploadIconBtn" class="btn btn-primary pull-right" style="margin: 10px 0px;display:none">Add icon</button>
                                                                <input type="file" name="file" id="file" class="filestyle" style="margin-bottom:25px" />
                                                            </div>

                                                        </div>
                                                    </div>


                                                </div>
                                                <div class="form-group">
                                                    <input type="submit" id="btnNatureofIncident" name="natureofinci" value="Add" class="btn btn-success add" @*onclick="submitNatureofIncident()";*@ />
