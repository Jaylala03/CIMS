                            <b class=""><i class="fa fa-sign-out" aria-hidden="true"></i> @Session["UserName"]</b>
                        </a>
                        <ul class="dropdown-menu dropdown-user animated flipInY">

                            <li><a href='@Url.Content("~/Account/LogOff")'> <i class="fa fa-power-off"></i> Logout</a></li>
                        </ul>
                        <!-- /.dropdown-user -->
                    </li>

                    <!-- /.dropdown -->
